  background-color: rgb(255, 255, 255);
  align-items: center;
  display: grid;
  grid-template-columns:50px 800px 50px 50px ;



}
.icon,.shop,.search{
  width: 16px;
}
.searchdiv,.shopdiv{

    margin-left: 30px;

}


